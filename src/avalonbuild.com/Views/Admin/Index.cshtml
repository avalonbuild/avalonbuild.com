@model avalonbuild.com.Models.Contact
@inject UserManager<ApplicationUser> UserManager

@{
    ViewBag.Title = "Administration";
    ViewBag.Description = "Administration";
}

@section Styles {
	<link rel="stylesheet" href="~/css/pages/admin.css")">
}

<section id="admin">

	<div class="container">

		<header>

			<h2>@ViewBag.Title</h2>

		</header>

		<ul>
			<li><a asp-controller="Account" asp-action="ChangePassword">Change Password</a></li>
			<li><a asp-controller="Users" asp-action="Index">Manage Users</a></li>
			<li><a asp-controller="Account" asp-action="LogOff">Log Off</a></li>
		</ul>

	</div>

</section>